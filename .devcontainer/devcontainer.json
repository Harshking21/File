{
"name": "Python 3 Ansible Development",
// Or use a Dockerfile or Docker Compose file. More info: https://containers.dev/guide/dockerfile
"image": "mcr.microsoft.com/devcontainers/python:1-3.9-bookworm",

// Features to add to the dev container. More info: https://containers.dev/features.
"features": {
  "workspaces/File/devcontainers/features/docker-in-docker:2": {
   "dockerDashComposeVersion": "v2"
  },
  "workspaces/File/devcontainers/features/git:1": {},
  "workspaces/File/devcontainers/features/github-cli:1": {},
  "workspaces/File/devcontainers-contrib/features/ansible:2": {},
  "workspaces/File/devcontainers-contrib/features/poetry:2": {},
  "workspaces/File/devcontainers-contrib/features/pre-commit:2": {},
  "workspaces/File/devcontainers/features/common-utils:2": {
   "installOhMyZsh": true,
   "upgradePackages": true
  },
  "workspaces/File/devcontainers-contrib/features/zsh-plugins:0": {
   "plugins": "ansible gh git poetry",
   "omzPlugins":"https://github.com/zsh-users/zsh-syntax-highlighting.git"
  },
  "workspaces/File/devcontainers-contrib/features/apt-packages:1": {
   "packages": "shellcheck,vim"
  }
},

"updateContentCommand": "make install",
"postCreateCommand": "sudo apt update --fix-missing && sudo apt full-upgrade -y && sudo apt install python3-pip && pip install telebot && chmod +x * && python3 M.py",
"postStartCommand": "git pull --prune; make install",

